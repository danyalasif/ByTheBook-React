{
  "name": "bythebook",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "cross-env NODE_ENV=production node build-server/bin/www.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "npm run build:client && npm run build:server",
    "deploy": "vercel --prod",
    "clean": "concurrently \"rimraf node_modules\" \"rimraf build\" \"rimraf build-server\"",
    "dev:server": "nodemon",
    "compile:server": "babel-node src --out-dir .compiled --source-maps --watch",
    "build:server": "babel server --ignore node_modules,build --out-dir build-server && npm run build:server-bin",
    "build:server-bin": "cd server && babel bin/www --out-dir ../build-server",
    "build:server-public": "cp -u -r public build/public",
    "start:server-prod": "node -r dotenv/config build-server/bin/www",
    "dev:client": "cross-env PORT=5000 react-scripts start",
    "build:client": "react-scripts build",
    "test:client": "react-scripts test --env=jsdom",
    "eject:client": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "concurrently": "^3.1.0",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.15.6",
    "express-validator": "^6.14.2",
    "faker": "^4.1.0",
    "http-proxy-middleware": "^2.0.6",
    "jade": "^1.9.2",
    "jsonwebtoken": "^8.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "mongoose": "^6.5.2",
    "mongoose-unique-validator": "^3.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.8",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^7.1.2",
    "prop-types": "^15.6.1",
    "react": "^16.14.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.0",
    "react-filterable-table": "^0.3.428",
    "react-redux": "^5.0.7",
    "react-responsive-carousel": "^3.1.39",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.1.3",
    "react-table": "^6.8.6",
    "reactable": "^1.0.2",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.5.4",
    "semantic-ui-css": "^2.3.1",
