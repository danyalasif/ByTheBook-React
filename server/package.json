{
    "name": "server",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "nodemon --exec  babel-node -- ./bin/www",
        "compile": "babel-node src --out-dir .compiled --source-maps --watch",
        "build": "babel . --ignore node_modules,build --out-dir build && npm run build:bin && npm run build:public",
        "build:bin": "babel ./bin/www --out-dir ./build",
        "build:public": "cp -u -r public build/public",
        "start:prod": "node -r dotenv/config ./build/bin/www"
    },
    "dependencies": {
        "axios": "^0.18.1",
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.19.0",
        "connect-mongo": "^2.0.3",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^6.2.0",
        "express": "~4.15.5",
        "express-session": "^1.17.0",
        "express-validator": "^5.3.1",
        "jade": "~1.11.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.7.12",
        "mongoose-unique-validator": "^2.0.3",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "nodemailer": "^4.7.0",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^5.0.1",
        "request": "^2.88.0",
        "request-promise-native": "^1.0.8",
        "serve-favicon": "~2.4.5",
        "socket.io": "^2.3.0",
        "uuid": "^3.3.3",
        "xml2js": "^0.4.22"
    },
    "devDependencies": {
        "nodemon": "^2.0.1"
    }
}
