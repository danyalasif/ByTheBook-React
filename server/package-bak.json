{
    "name": "server",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "nodemon --exec  babel-node -- ./bin/www",
        "compile": "babel-node src --out-dir .compiled --source-maps --watch",
        "build": "babel . --ignore node_modules,build --out-dir build && npm run build:bin && npm run build:public",
        "build:bin": "babel ./bin/www --out-dir ./build",
        "build:public": "cp -u -r public build/public",
        "start:prod": "node -r dotenv/config ./build/bin/www"
    },
    "dependencies": {
        "axios": "^0.27.2",
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.3",
        "connect-mongo": "^4.6.0",
        "cookie-parser": "~1.4.3",
        "cors": "^2.8.4",
        "debug": "^4.3.4",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "express-session": "^1.15.6",
        "express-validator": "^6.14.2",
        "jade": "^1.9.2",
        "jsonwebtoken": "^8.2.1",
        "mongoose": "^6.5.2",
        "mongoose-unique-validator": "^3.1.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.7.8",
        "passport": "^0.6.0",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^7.1.2",
        "request": "^2.87.0",
        "request-promise-native": "^1.0.5",
        "serve-favicon": "^2.5.0",
        "socket.io": "^4.5.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "nodemon": "^2.0.1"
    }
}
